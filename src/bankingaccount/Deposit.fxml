<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="depositView" prefHeight="400.0" prefWidth="600.0" 
            xmlns:fx="http://javafx.com/fxml/1" 
            xmlns="http://javafx.com/javafx/2.2" 
            fx:controller="bankingaccount.DepositController">
  <children>
    <Label alignment="CENTER" layoutX="141.0" layoutY="0.0" prefHeight="77.0" 
           prefWidth="286.0" text="Deposit" textAlignment="LEFT">
      <font>
        <Font size="30.0" />
      </font>
    </Label>
    <Label layoutX="241.0" layoutY="173.0" prefHeight="37.0" 
           prefWidth="81.0" text="Amount:">
      <font>
        <Font size="18.0" fx:id="x1" />
      </font>
    </Label>
    <TextField id="amouttf" fx:id="amounttf" layoutX="209.0" layoutY="217.0" />
    <Button layoutX="60.0" layoutY="275.0" mnemonicParsing="false" 
            onAction="#handleReturn" prefHeight="70.0" prefWidth="100.0" 
            text="Return" />
    <Button layoutX="427.0" layoutY="275.0" mnemonicParsing="false" 
            onAction="#handleSubmit" prefHeight="70.0" prefWidth="100.0" 
            text="Ok" />
    <Label alignment="CENTER" font="$x1" layoutX="113.0" layoutY="84.0" 
           prefHeight="70.0" prefWidth="137.0" text="Your Balance:" />
    <Label fx:id="balancetf" alignment="CENTER" font="$x1" layoutX="271.0" 
           layoutY="91.0" prefHeight="56.0" prefWidth="221.0" text="0.0" />
  </children>
</AnchorPane>
